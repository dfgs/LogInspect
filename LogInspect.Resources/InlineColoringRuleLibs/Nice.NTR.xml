<?xml version="1.0"?>
<InlineColoringRuleLib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" NameSpace="Nice.NTR">
  <Items>
    <InlineColoringRule Name="CVSKey" Pattern="CVSKEY" Foreground="Black" Bold="true"/>
    <InlineColoringRule Name="CVSKeyID" Pattern="(?&lt;=CVSKEY=){Number}" Foreground="Purple"/>
    <InlineColoringRule Name="DRLinkTimeSlot" Pattern="DR_S{Number}_B{Number}_TS{Number}" Foreground="Olive" Underline="false" Bold="false" Italic="true" />

    <InlineColoringRule Name="Target" Pattern=" target" IgnoreCase="true" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="TargetID" Pattern="(?&lt;=target:? *){Number}" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="TargetID2" Pattern="(?&lt;=\[){Number}(?=\])" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Device" Pattern=" device" IgnoreCase="true" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="DeviceID" Pattern="(?&lt;=device:? *){Number}" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Reference" Pattern="reference" IgnoreCase="true" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="ReferenceID" Pattern="(?&lt;=reference:? *){Number}" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />

    <InlineColoringRule Name="CallID" Pattern="CallId" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="CallIDValue" Pattern="(?&lt;=CallId: ){HexNumber}" Foreground="Purple" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="CallState" Pattern="CallState" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="CallStateValue" Pattern="(?&lt;=CallState: ){Word}" Foreground="GoldenRod" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="LocationState" Pattern="StateOfLocation" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="MediumState" Pattern="StateOfMedium" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="LocationStateOK" Pattern="(?&lt;=StateOfLocation : )Connected" Foreground="Green" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="LocationStateNOK" Pattern="(?&lt;=StateOfLocation : )[^ ]+" Foreground="Red" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="MediumStateOK" Pattern="(?&lt;=StateOfMedium : )CorrectMediumFound" Foreground="Green" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="MediumStateNOK" Pattern="(?&lt;=StateOfMedium : )[^ ]+" Foreground="Red" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Generated" Pattern="Generated" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="Completed" Pattern="Completed" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="GeneratedCount" Pattern="(?&lt;=Generated ){Number}" Foreground="SteelBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="CompletedCount" Pattern="(?&lt;=Completed ){Number}" Foreground="SteelBlue" Underline="false" Bold="false" Italic="false" />
		<InlineColoringRule Name="FPSMessage" Pattern="&lt;\?xml version=&quot;1\.0&quot;\?&gt;&lt;recorder[\s\S]*?&lt;\/recorder&gt;" Foreground="Blue" Underline="true" Bold="false" Italic="false" DocumentType="Xml" />

		</Items>
</InlineColoringRuleLib>