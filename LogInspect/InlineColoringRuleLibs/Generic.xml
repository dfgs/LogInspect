<?xml version="1.0"?>
<InlineColoringRuleLib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" NameSpace="Nice.NTR">
  <Items>
    <InlineColoringRule Name="Const" Pattern="{Const}" Foreground="Brown" Bold="true"  />
    <InlineColoringRule Name="UNCPath" Pattern="{UNCPath}" Foreground="Blue" Underline="true" Bold="false" Italic="false" />
    <InlineColoringRule Name="QuotedString" Pattern="'[^']+'" Foreground="Black" Underline="false" Bold="false" Italic="true" />
  </Items>
</InlineColoringRuleLib>