<?xml version="1.0"?>
<InlineColoringRuleLib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" NameSpace="Nice.NIM">
  <Items>
    <InlineColoringRule Name="Logger" Pattern="L(og)?ge?r{IDSuffix}}" Foreground="Black" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="Interaction" Pattern="interaction{IDSuffix}" IgnoreCase="true" Foreground="BlueViolet" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Compound" Pattern="compound{IDSuffix}" IgnoreCase="true" Foreground="BlueViolet" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="CLSCall" Pattern="CLSCall{IDSuffix}" IgnoreCase="true" Foreground="BlueViolet" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Channel" Pattern="Ch={Number}" Foreground="Black" IgnoreCase="false" Underline="false" Bold="true" Italic="false" />
    <InlineColoringRule Name="Device" Pattern="D(N|EV)=&quot;{Number}&quot; " Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Extension" Pattern="(extension|station){IDSuffix}" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
    <InlineColoringRule Name="Agent" Pattern="(agent|user){IDSuffix}" IgnoreCase="true" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
  </Items>
</InlineColoringRuleLib>