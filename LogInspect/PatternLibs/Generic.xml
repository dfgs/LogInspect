<?xml version="1.0"?>
<PatternLib xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" NameSpace="Generic">
  <Items>
    <Pattern Name="Const" Value="[A-Z]+(_[A-Z]+)+"/>
    <Pattern Name="EmptyLine" Value="^$" />
    <Pattern Name="Comment" Value="^//.*$" />
    <Pattern Name="WholeLine" Value="^.*$" />
    <Pattern Name="Spaces" Value=" +" />
    <Pattern Name="Date" Value="\d\d\d\d-\d\d-\d\d" />
    <Pattern Name="Time" Value="\d\d?:\d\d:\d\d(\.\d+)?( (A|P)?M)?"  />
    <Pattern Name="Number" Value="\d+" />
    <Pattern Name="RestOfTheLine" Value=".*$" />
    <Pattern Name="Word" Value="\w+" />
  </Items>
</PatternLib>