<?xml version="1.0"?>
<FormatHandler xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" SeverityColumn="Severity" TimeStampColumn="Date">
  <Name>Nice.NTR.Alcatel-Lucent</Name>
  <FileNamePattern>^Alcatel-Lucent-\d+-\d\d\d\d-\d\d-\d\d_\d\dhr\.log$</FileNamePattern>
  <AppendLineToPreviousPatterns />
  <AppendLineToNextPatterns />
  <DiscardLinePatterns>
    <string>^$</string>
  </DiscardLinePatterns>
  <Columns>
    <Column Name="Date" Type="DateTime" Format="yyyy-MM-dd HH:mm:ss.fff" Width="200" Alignment="Center" IsFilterItemSource="false">
      <InlineColoringRules />
    </Column>
    <Column Name="Severity" Width="100" Alignment="Center" IsFilterItemSource="true">
      <InlineColoringRules />
    </Column>
    <Column Name="Event" Width="300" IsFilterItemSource="true">
      <InlineColoringRules />
    </Column>
    <Column Name="Message" Width="600" IsFilterItemSource="false">
      <InlineColoringRules>
        <InlineColoringRule Pattern="(\[|')DR_S\d+_B\d+_TS\d+(\]|')" Foreground="BlueViolet" Underline="false" Bold="false" Italic="true" />
        <InlineColoringRule Pattern="[A-Z]+(_[A-Z]+)+" Foreground="Brown" Underline="false" Bold="true" Italic="false" />
        <InlineColoringRule Pattern="\[\d+\]" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
        <InlineColoringRule Pattern="(T|t)arget:? *\d+" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
        <InlineColoringRule Pattern="(D|d)evice:? *\d+" Foreground="CornflowerBlue" Underline="false" Bold="false" Italic="false" />
        <InlineColoringRule Pattern="CallState: \w+" Foreground="Chocolate" Underline="false" Bold="false" Italic="false" />
        <InlineColoringRule Pattern="\[SUCCESS\]" Foreground="Green" Underline="false" Bold="true" Italic="false" />
      </InlineColoringRules>
    </Column>
  </Columns>
  <Rules>
    <Rule Name="NTR Event" Discard="false">
      <Tokens>
        <Token Pattern="\d\d\d\d-\d\d-\d\d \d\d:\d\d:\d\d\.\d+" Name="Date" />
        <Token Pattern=" +" />
        <Token Pattern="\w+" Name="Severity" />
        <Token Pattern=" +" />
        <Token Pattern="\[" />
        <Token Pattern="[^\]]+" Name="Event" />
        <Token Pattern="\] " />
        <Token Pattern=".+" Name="Message" />
      </Tokens>
    </Rule>
    <Rule Name="CSTA Event" Discard="false">
      <Tokens>
        <Token Pattern="\d\d\d\d-\d\d-\d\d \d\d:\d\d:\d\d\.\d+" Name="Date" />
        <Token Pattern=" +" />
        <Token Pattern="\w+" Name="Severity" />
        <Token Pattern=" +" />
        <Token Pattern="CSTA EVENT - Class \d+ - Type \d+" Name="Event" />
      </Tokens>
    </Rule>
    <Rule Name="NTR Message" Discard="false">
      <Tokens>
        <Token Pattern="\d\d\d\d-\d\d-\d\d \d\d:\d\d:\d\d\.\d+" Name="Date" />
        <Token Pattern=" +" />
        <Token Pattern="\w+" Name="Severity" />
        <Token Pattern=" +" />
        <Token Pattern=".+" Name="Message" />
      </Tokens>
    </Rule>
    <Rule Name="Default" Discard="false">
      <Tokens>
        <Token Pattern=".*" Name="Message" />
      </Tokens>
    </Rule>
  </Rules>
  <EventColoringRules>
    <EventColoringRule Column="Severity" Pattern="ERROR" Background="OrangeRed" />
    <EventColoringRule Column="Severity" Pattern="WARN" Background="Orange" />
  </EventColoringRules>
</FormatHandler>